	<queryString>
		<![CDATA[SELECT
    l.titulo AS "Título del Libro",
    COUNT(e.idejemplar) AS "Total de Ejemplares",
    SUM(CASE WHEN e.estado = true THEN 1 ELSE 0 END) AS "Ejemplares Disponibles"
FROM
    libros l
JOIN
    ejemplar e ON l.idlibro = e.idlibro
GROUP BY
    l.idlibro, l.titulo
ORDER BY
    l.titulo;]]>
	</queryString>
