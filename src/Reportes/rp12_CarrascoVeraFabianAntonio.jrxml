	<queryString>
		<![CDATA[SELECT
    A.nombres || ' ' || A.apepaterno || ' ' || A.apematerno AS "Autor",
    COUNT(ALA.idlibro) AS "Total_Titulos_en_Inventario"
FROM
    AUTOR A
JOIN
    ASIGNAR_LIBRO_AUTOR ALA ON A.idautor = ALA.idautor
GROUP BY
    A.idautor, "Autor"
ORDER BY
    "Total_Titulos_en_Inventario" DESC
LIMIT 10;]]>
	</queryString>
