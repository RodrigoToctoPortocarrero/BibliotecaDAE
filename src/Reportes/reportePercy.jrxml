	<parameter name="NOMBRE_EDITORIAL" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    E.nombre AS Nombre_Editorial,
    L.titulo AS Titulo_Libro,
    L.aniopublicacion AS Anio_Publicacion
FROM
    LIBROS L
JOIN
    EDITORIAL E ON L.ideditorial = E.ideditorial
WHERE
    E.nombre ILIKE '%' || $P{NOMBRE_EDITORIAL} || '%'
ORDER BY
    L.titulo;]]>
	</queryString>
