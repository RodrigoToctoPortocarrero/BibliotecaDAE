	<queryString>
		<![CDATA[select pre.fechaprestamo as fechaprestamo, pre.fechadevolucion as fechaLimite,
dev.fechadevolucionreal as fechadevreal ,usu.nombre ||' '|| usu.ap_paterno ||' '|| usu.ap_materno as
lector, dev.multagenerada as multa from devolucion as dev INNER JOIN prestamo as pre ON
pre.idprestamo = dev.iddevolucion INNER JOIN usuario as usu ON usu.idusuario = pre.idusuariolector
where dev.multagenerada > 0 order by dev.multagenerada desc;]]>
	</queryString>
