	<queryString>
		<![CDATA[select pre.idprestamo as idprestamo,usu.nombre as
lector, dev.multagenerada as multa from devolucion as dev INNER JOIN prestamo as pre ON
pre.idprestamo = dev.iddevolucion INNER JOIN usuario as usu ON usu.idusuario = pre.idusuariolector
where dev.multagenerada > 0 order by pre.idprestamo;]]>
