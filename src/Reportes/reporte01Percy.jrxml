	<queryString>
		<![CDATA[SELECT
    L.titulo AS Titulo_Libro,
    L.aniopublicacion AS Anio_Publicacion,
    C.nombrecategoria AS Nombre_Categoria,
    E.nombre AS Nombre_Editorial
FROM
    LIBROS L
JOIN
    CATEGORIA C ON L.idcategoria = C.idcategoria
JOIN
    EDITORIAL E ON L.ideditorial = E.ideditorial
ORDER BY
    L.titulo, C.nombrecategoria;]]>
	</queryString>
