	<queryString>
		<![CDATA[SELECT
    u.tipousuario AS "Tipo de Usuario",
    CASE WHEN u.estado = true THEN 'Activo' ELSE 'Inactivo' END AS "Estado",
    COUNT(u.idusuario) AS "Cantidad de Usuarios"
FROM
    usuario u
GROUP BY
    u.tipousuario,
    u.estado
ORDER BY
    u.tipousuario;]]>
	</queryString>
