	<field name="detalle_fechas" class="java.lang.String"/>
	<field name="dias_de_retraso" class="java.lang.Integer"/>
	<field name="motivo_multa" class="java.lang.String"/>
	<field name="monto_multa" class="java.math.BigDecimal"/>
