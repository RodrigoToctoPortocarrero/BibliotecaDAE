<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rp14tocto" language="groovy" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="7c33f805-dead-43ba-a95c-4a02f6389d2b">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="fechainicio" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fechafinal" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    u.nombre || ' ' || u.ap_paterno || ' ' || u.ap_materno AS lector,

    'Préstamo: ' || p.fechaprestamo::date ||
    ' | Límite: ' || p.fechadevolucionestimada::date ||
    ' | Devuelto: ' || d.fechadevolucionreal::date
    AS detalle_fechas,

    (d.fechadevolucionreal::date - p.fechadevolucionestimada::date) AS dias_de_retraso,

    dd.observaciones AS motivo_multa,
    dd.multa AS monto_multa,

    CASE
        WHEN m.pagado = TRUE THEN 'Sí'
        ELSE 'No'
    END AS multa_pagada,

    m.fechagenerada::date AS fecha_multa_generada

FROM MULTA m
INNER JOIN USUARIO u ON u.idusuario = m.idusuario
INNER JOIN PRESTAMO p ON p.idprestamo = m.idprestamo
INNER JOIN DEVOLUCION d ON d.idprestamo = p.idprestamo
INNER JOIN DETALLE_DEVOLUCION dd ON dd.iddevolucion = d.iddevolucion

WHERE m.fechagenerada::date
      BETWEEN $P{fechainicio}::date AND $P{fechafinal}::date

ORDER BY m.fechagenerada::date;
]]>
	</queryString>
	<field name="lector" class="java.lang.String"/>
	<field name="detalle_fechas" class="java.lang.String"/>
	<field name="dias_de_retraso" class="java.lang.Integer"/>
	<field name="motivo_multa" class="java.lang.String"/>
	<field name="monto_multa" class="java.math.BigDecimal"/>
	<field name="multa_pagada" class="java.lang.String"/>
	<field name="fecha_multa_generada" class="java.sql.Date"/>
