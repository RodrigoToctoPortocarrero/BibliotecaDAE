	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="fechainicio" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fechafinal" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    u.nombre || ' ' || u.ap_paterno || ' ' || u.ap_materno AS lector,

    'Préstamo: ' || p.fechaprestamo::date ||
    ' | Límite: ' || p.fechadevolucionestimada::date ||
    ' | Devuelto: ' || d.fechadevolucionreal::date
    AS detalle_fechas,

    (d.fechadevolucionreal::date - p.fechadevolucionestimada::date) AS dias_de_retraso,

    dd.observaciones AS motivo_multa,
    dd.multa AS monto_multa

FROM MULTA m
INNER JOIN USUARIO u ON u.idusuario = m.idusuario
INNER JOIN PRESTAMO p ON p.idprestamo = m.idprestamo
INNER JOIN DEVOLUCION d ON d.idprestamo = p.idprestamo
INNER JOIN DETALLE_DEVOLUCION dd ON dd.iddevolucion = d.iddevolucion

WHERE m.fechagenerada::date
      BETWEEN $P{fechainicio}::date AND $P{fechafinal}::date and dd.multa > 0

ORDER BY m.fechagenerada::date;]]>
	</queryString>
