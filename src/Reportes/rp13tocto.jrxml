	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<queryString>
		<![CDATA[SELECT
    u.nombre || ' ' || u.ap_paterno || ' '|| u.ap_materno AS Lector,
    COUNT(*) AS cantidad_de_retrasos,
    MAX(info.dias_retraso) AS mayor_cantidad_dias_retraso,
    STRING_AGG(
        'Prestamo: ' || info.fechaprestamo
        || ' | Limite: ' || info.fechadev_estimada
        || ' | Devuelto: ' || info.fechadev_real,
        ' || '
    ) AS detalle_fechas
FROM (
    SELECT
        d.iddevolucion,
        p.idusuariolector,
        p.fechaprestamo,
        p.fechadevolucionestimada AS fechadev_estimada,
        d.fechadevolucionreal AS fechadev_real,
        (d.fechadevolucionreal - p.fechadevolucionestimada) AS dias_retraso
    FROM DEVOLUCION d
    INNER JOIN PRESTAMO p ON p.idprestamo = d.idprestamo
    WHERE d.fechadevolucionreal > p.fechadevolucionestimada
) info
INNER JOIN USUARIO u ON u.idusuario = info.idusuariolector
GROUP BY u.idusuario, Lector
ORDER BY cantidad_de_retrasos DESC
LIMIT 3;]]>
	</queryString>
