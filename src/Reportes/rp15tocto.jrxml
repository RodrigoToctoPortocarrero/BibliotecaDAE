	<field name="mes_numero" class="java.lang.String"/>
	<field name="mes_nombre" class="java.lang.String"/>
	<field name="cantidad_multas" class="java.lang.Long"/>
	<field name="monto_total_generado" class="java.math.BigDecimal"/>
