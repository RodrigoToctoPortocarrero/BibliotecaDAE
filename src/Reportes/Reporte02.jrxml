	<parameter name="FECHA_INICIO" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA_FIN" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    L.idlibro AS id_Libro,
    L.titulo AS Titulo_Libro,
    COUNT(DP.idejemplar) AS Total_Prestamos
FROM
    LIBROS L
JOIN
    EJEMPLAR E ON L.idlibro = E.idlibro
JOIN
    DETALLE_PRESTAMO DP ON E.idejemplar = DP.idejemplar
JOIN
    PRESTAMO P ON DP.idprestamo = P.idprestamo
WHERE
    P.fechaprestamo >= $P{FECHA_INICIO}
    AND P.fechaprestamo <= $P{FECHA_FIN}
GROUP BY
    L.idlibro,
    L.titulo
ORDER BY
    Total_Prestamos DESC, Titulo_Libro ASC;]]>
	</queryString>
	<field name="id_libro" class="java.lang.Integer"/>
	<field name="titulo_libro" class="java.lang.String"/>
	<field name="total_prestamos" class="java.lang.Long"/>
