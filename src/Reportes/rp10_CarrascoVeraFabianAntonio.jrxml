	<parameter name="nombrecategoria" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    L.titulo,
    C.nombrecategoria,
    COUNT(P.idprestamo) AS "Veces_Prestado"
FROM
    LIBROS L
JOIN
    CATEGORIA C ON L.idcategoria = C.idcategoria
JOIN
    EJEMPLAR E ON L.idlibro = E.idlibro
JOIN
    DETALLE_PRESTAMO DP ON E.idejemplar = DP.idejemplar
JOIN
    PRESTAMO P ON DP.idprestamo = P.idprestamo
WHERE
    C.nombrecategoria = $P{nombrecategoria}
GROUP BY
    L.titulo, C.nombrecategoria
ORDER BY
    "Veces_Prestado" DESC;]]>
	</queryString>
